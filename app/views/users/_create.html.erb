<div class="create-user">
<%= form_for :user, url: create_user_path do |f| %>
    <%= f.label :email %>
    <%= f.text_field :email %><br>

    <%= f.label :password %>
    <%= f.password_field :password %><br>

    <%= f.label :password_confirmation %>
    <%= f.password_field :password_confirmation %>

    <%= f.submit "Create Account" %>
  <% end %>
</div>
